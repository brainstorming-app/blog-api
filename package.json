    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": "./node_modules/babel/bin/babel-node index.js"
  },
  "repository": {
    "type": "git",
