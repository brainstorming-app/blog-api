    "mocha-traceur": "^2.1.0",
    "pg": "^4.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.3.2",
    "sequelize-cli": "^1.7.1",
    "sqlite3": "^3.0.8",
    "winston": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.6.7",
    "mocha": "*",
    "supertest": "^1.0.1",
    "should": ">= 0.0.1",
